{
  "name": "bull-board",
  "description": "Bull queue UI for inspecting jobs",
  "version": "1.0.0-alpha.7",
  "keywords": [
    "bull",
    "redis",
    "queue",
    "monitoring"
  ],
  "author": "Vitor Capretz <capretzvitor@gmail.com>",
  "contributors": [
    "Erik Engervall <erik.engervall@gmail.com>"
  ],
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vcapretz/bull-board.git"
  },
  "bugs": {
    "url": "https://github.com/vcapretz/bull-board/issues"
  },
  "files": [
    "dist/**/*",
    "static/**/*"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "lint": "eslint \"./src/**/*.ts*\"",
    "build": "yarn build:clean && yarn build:ui && yarn build:routes && cp ./src/ui/index.ejs dist/ui/index.ejs",
    "build:ui": "NODE_ENV=production webpack",
    "build:ui:watch": "webpack --watch",
    "build:routes": "yarn tsc",
    "build:clean": "rm -rf ./static ./dist",
    "start:dev:docker": "docker-compose up -d",
    "start:dev:server": "nodemon example.js",
    "start:dev": "yarn start:dev:docker && yarn start:dev:server",
    "test": "yarn ts-node src/dockest"
  },
  "dependencies": {
    "@types/bull": "^3.12.2",
    "@types/express": "^4.17.6",
    "@types/react-dom": "^16.9.5",
    "date-fns": "2.12.0",
    "ejs": "3.0.1",
    "express": "4.17.1",
    "pretty-bytes": "5.3.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-highlight": "0.12.0",
    "redis-info": "^3.0.7"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/pretty-bytes": "^5.2.0",
    "@types/react-highlight": "^0.12.2",
    "@types/redis-info": "^3.0.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/typescript-estree": "^2.30.0",
    "babel-preset-react-app": "^7.0.2",
    "bull": "^3.13.0",
    "bullmq": "^1.8.7",
    "css-loader": "^3.5.3",
    "dockest": "2.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.5.1",
    "nodemon": "^2.0.3",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "react-dev-utils": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
